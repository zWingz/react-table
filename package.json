{
  "name": "@zzwing/react-table",
  "version": "1.4.1",
  "description": "A react fixed table component",
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "./index.d.ts",
  "keywords": [
    "react",
    "table",
    "fixed",
    "fixed-table",
    "component"
  ],
  "files": [
    "dist",
    "es",
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "docz": "docz dev",
    "docz:build": "docz build",
    "build": "rollup -c",
    "tslint": "tslint --fix --format codeFrame --project .",
    "dev": "poi --serve",
    "pub": "yarn tslint && yarn build && yarn publish --access=public",
    "test": "jest"
  },
  "author": "zhengzwing@gmail.com",
  "license": "MIT",
  "homepage": "https://zwing.site/react-table/#/",
  "repository": {
    "type": "git",
    "url": "https://github.com/zWingz/react-table.git"
  },
  "bugs": {
    "url": "https://github.com/zWingz/react-table/issues"
  },
  "peerDependencies": {
    "prop-types": ">=15.6.2",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@poi/plugin-typescript": "^12.0.0",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^24.0.9",
    "@types/node": "^11.10.4",
    "@types/react": "^16.4.16",
    "autoprefixer": "^9.1.5",
    "cz-conventional-changelog": "^2.1.0",
    "docz": "^0.13.7",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^0.13.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "jest": "^24.1.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.9.2",
    "poi": "^12.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "rollup": "^1.4.1",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|scss)$": "<rootDir>/test/utils.ts"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/__test__/**",
      "!src/**/module.d.ts"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__test__/*.test.(ts|tsx)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
